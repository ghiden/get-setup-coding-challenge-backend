{
  "name": "get-setup-backend",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "dotenv -e .env.development nodemon src/index.ts",
    "build": "tsc",
    "lint": "eslint --fix src test",
    "migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "seed:dev": "dotenv -e .env.development -- prisma db seed",
    "prisma-studio:dev": "dotenv -e .env.development prisma studio -- -b 'Google Chrome'",
    "prisma-studio:test": "dotenv -e .env.test prisma studio -- -p 5556 -b 'Google Chrome'",
    "test": "dotenv -e .env.test jest -i"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "private": true,
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
